{
  "name": "hello-world",
  "version": "1.0.0",
  "description": "Hello world app to run CI/CD pipeline",
  "main": "src/index.js",
  "scripts": {
    "start": "node src",
    "test": "jest --coverage",
    "sonar": "node sonar-project.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shrisais/hello-world.git"
  },
  "author": "Shri Sai Sadhana Natarajan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shrisais/hello-world/issues"
  },
  "homepage": "https://github.com/shrisais/hello-world#readme",
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "jest": "^28.1.1",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.8.1",
    "supertest": "^6.2.3"
  }
}
